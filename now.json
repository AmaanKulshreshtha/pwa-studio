{
    "version": 2,
    "name": "venia",
    "alias": [
        "magento-pwastudio-venia-develop"
    ],
    "scope": "magento-research",
    "env": {
        "UPWARD_JS_BIND_LOCAL": "false",
        "IMAGE_OPTIMIZING_ORIGIN": "backend",
        "MAGENTO_BACKEND_URL": "https://release-dev-231-npzdaky-zddsyhrdimyra.us-4.magentosite.cloud",
        "NODE_ENV": "production"
    },
    "build": {
        "env": {
            "MAGENTO_BACKEND_URL": "https://release-dev-231-npzdaky-zddsyhrdimyra.us-4.magentosite.cloud"
        }
    },
    "builds": [
        {
            "src": "packages/venia-concept/package.json",
            "use": "@now/static-build"
        },
        {
            "config": {
                "includeFiles": [
                    "package.json",
                    ".env",
                    "upward.yml",
                    "node_modules/@magento/venia-ui/lib/queries/*",
                    "static/*",
                    "templates/*"
                ],
                "maxLambdaSize": "20mb"
            },
            "src": "packages/venia-concept/server-lambda.js",
            "use": "@now/node@0.11.1"
        }
    ],
    "routes": [
        {
            "src": "^/static/(.*)",
            "dest": "packages/venia-concept/static/$1"
        },
        {
            "src": "^/(.*\\.(?:js|json|svg|png|ico|txt))",
            "dest": "packages/venia-concept/$1"
        },
        {
            "src": "/(.*)",
            "dest": "packages/venia-concept/server-lambda.js"
        }
    ],
    "regions": [
        "sfo1",
        "bru"
    ],
    "public": true
}
